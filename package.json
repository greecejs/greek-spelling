{
  "name": "greek-spelling",
  "version": "0.1.0",
  "description": "Greek spelling alphabet",
  "main": "dist/greekSpelling.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "rollup -c",
    "test": "ava --require babel-register",
    "lint": "eslint .",
    "cover": "nyc --reporter=lcov --reporter=text --check-coverage true --branches 100 --functions 100 --lines 100 --statements 100 npm t",
    "ci": "npm run lint && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greecejs/greek-spelling.git"
  },
  "keywords": [
    "greece",
    "greek",
    "spelling",
    "phonetic",
    "alphabet"
  ],
  "author": {
    "name": "Thanos Lefteris",
    "email": "alefteris@gmail.com",
    "url": "http://thanoslefteris.com/"
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/greecejs/greek-spelling/issues"
  },
  "homepage": "https://github.com/greecejs/greek-spelling#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^2.6.0",
    "nyc": "^6.1.1",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-uglify": "^0.3.1"
  }
}
